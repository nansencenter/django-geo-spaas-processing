-o collection = mfs_med-cmcc-cur
-i time_variable = time
   lon_variable = lon
   lat_variable = lat
   depth_variable = depth
   depth_dimension = depth
   depth_value = 1.018237
   depth_tolerance = 0.5
   variables = uo,vo
   time_coverage_relative_start = -1800
   time_coverage_relative_end = +1800
